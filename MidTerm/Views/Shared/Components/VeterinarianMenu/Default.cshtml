<!--Note: Componet that will create a dynamic drop down menu-->
<li class="nav-item dropdown">
    <a class="nav-link dropdown-toggle" role="button" data-bs-toggle="dropdown">Veterinarians</a>
    <ul class="dropdown-menu">
        @foreach (var veterinarian in Model)
        {
            <li> <a asp-controller="Veterinarian" asp-action="Details" asp-route-id="@veterinarian.Id" class="text-decoration-none text-reset">@veterinarian.Name</a> </li>
        }
    </ul>
</li>